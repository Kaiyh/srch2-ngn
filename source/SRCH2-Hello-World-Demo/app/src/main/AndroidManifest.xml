<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.srch2.android.demo.helloworld"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
    

	<uses-permission android:name="android.permission.INTERNET" /> 
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
                <activity
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden|screenSize|locale"
            android:name="com.srch2.android.demo.helloworld.MainActivity"
            android:label="SRCH2 Hello World Demo" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.srch2.android.http.service.SRCH2Service"
            android:exported="false"
            android:enabled="true"
            android:process=":remotee">
        </service>

    </application>

</manifest>
